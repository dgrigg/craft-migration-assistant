{
  "name": "dgrigg/craft-migration-assistant",
  "description": "Create migrations for settings and content at the click of a mouse. Keep environments in sync with ease.",
  "version": "4.0.0-beta.1",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft plugin",
    "migration",
    "migration manager",
    "migration assistant"
  ],
  "authors": [
    {
      "name": "Derrick Grigg",
      "email": "derrick@dgrigg.com"
    }
  ],
  "support": {
    "docs": "https://github.com/dgrigg/craft-migration-assistant/blob/master/README.md",
    "issues": "https://github.com/dgrigg/craft-migration-assistant/issues"
  },
  "license": "proprietary",
  "type": "craft-plugin",
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "dgrigg\\migrationassistant\\": "src/"
    }
  },
  "extra": {
    "name": "Migration Assistant",
    "handle": "migrationassistant",
    "developer": "DGrigg Dev Inc.",
    "developerUrl": "https://dgrigg.com",
    "class": "dgrigg\\migrationassistant\\MigrationAssistant",
    "hasCpSettings": false,
    "hasCpSection": true,
    "changelogUrl": "https://raw.githubusercontent.com/dgrigg/craft-migration-assistant/master/CHANGELOG.md"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "craftcms/rector": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
